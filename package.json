{
  "name": "este",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "license": "MIT",
  "scripts": {
    "dev:server": "nodemon server.js",
    "dev:server:graphql": "nodemon -x node -r dotenv/config servergraphql.js",
    "dev:playground": "graphql playground --port 5000",
    "dev": "run-p dev:server dev:server:graphql dev:playground",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "deploy": "node ./scripts/deploy",
    "deploy:db": "yarn prisma deploy",
    "extract:messages": "yarn clean-babel-cache && rimraf lang/.messages && next build && node ./scripts/extractMessages",
    "favicon": "node ./scripts/favicon",
    "flow": "flow",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "prettier": "prettier --write '**/*.js'",
    "clean:babel:cache": "rimraf node_modules/.cache/babel-loader",
    "test": "run-s flow jest build",
    "deps": "rimraf node_modules yarn.lock flow-typed/npm && yarn && flow-typed install",
    "analyze": "cross-env ANALYZE=1 next build",
    "schema": "yarn graphql get-schema -p app -o schemagenerated.graphql",
    "relay": "yarn schema && rimraf **/__generated__ && relay-compiler --exclude '**/node_modules/**' '**/__mocks__/**' '**/__tests__/**' '**/__generated__/**' '**/flow-typed/**' --src ./ --schema backend/schema.graphql"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "ajv": "^6.0.0-rc.1",
    "array-equal": "^1.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "bcryptjs": "^2.4.3",
    "big.js": "^5.0.3",
    "color": "^3.0.0",
    "cookie": "^0.3.1",
    "create-react-context": "^0.1.3",
    "cross-env": "^5.0.1",
    "diacritics-map": "^0.1.0",
    "draft-js": "^0.10.4",
    "glamor": "^2.20.40",
    "glob": "^7.1.2",
    "graphql-yoga": "^1.2.1",
    "gravatar": "^1.6.0",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.1",
    "next": "^4.0.0",
    "nprogress": "^0.2.0",
    "path-match": "^1.2.4",
    "prisma-binding": "^1.5.3",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "ramda": "^0.25.0",
    "raven": "^2.1.1",
    "raven-js": "^3.17.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.3.0",
    "react-relay": "^1.5.0-rc.1",
    "react-x-ray": "^1.0.0-3",
    "relay-runtime": "^1.5.0-rc.1",
    "resize-observer-polyfill": "^1.5.0",
    "smoothscroll-polyfill": "^0.4.0",
    "styled-jsx": "^2.2.1",
    "url": "^0.11.0",
    "validator": "^9.4.0",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.1",
    "babel-plugin-relay": "^1.5.0-rc.1",
    "babel-plugin-transform-define": "^1.2.0",
    "dotenv": "^5.0.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.1.5",
    "graphql-cli": "^2.13.0",
    "jest": "^22.0.1",
    "nodemon": "^1.14.12",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.5.1",
    "prisma": "^1.1.2",
    "react-test-renderer": "^16.0.0",
    "relay-compiler": "^1.5.0-rc.1",
    "relay-devtools": "^1.4.0",
    "rimraf": "^2.4.5"
  }
}
