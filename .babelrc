{
  "env": {
    "development": {
      "presets": ["next/babel", "@babel/preset-flow"],
      "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        "inline-dotenv",
        "relay"
      ]
    },
    "production": {
      "presets": ["next/babel", "@babel/preset-flow"],
      "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        "transform-inline-environment-variables",
        "relay",
        ["react-intl", { "messagesDir": "lang/.messages/" }]
      ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-optional-chaining",
        "relay"
      ]
    }
  }
}
