{
  "stage": 2, // Draft + Candidate
  "optional": [
    // Stage 1 - Proposal
    "es7.classProperties",
    "es7.decorators",
    "es7.objectRestSpread",
    // Stage 0 - Strawman
    "es7.functionBind"
  ],
  "plugins": [
    "react-transform"
  ],
  "extra": {
    "react-transform": [{
      "target": "react-transform-webpack-hmr",
      "imports": ["react"],
      "locals": ["module"]
    }, {
      "target": "react-transform-catch-errors",
      // https://github.com/KeywordBrain/redbox-react/issues/11
      "imports": ["react", "delicate-error-reporter"]
    }]
  }
}
